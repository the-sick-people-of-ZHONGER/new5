void deeptraone(graphmatrix gm,int n)
{
  int i;
  gm.istrav[n]+1;
  System.out.printf("->5c",gm.vertex[n]);
  for(i=0;I<gm.vertexnum;i++)
  {
    if(gm.edgeweight[n][i]!=graphmatrix.maxvalue&&gm.istrav[n]==0)
    {
      deeptraone(gm,i);
    }
  }
}
void  deeptragraph(graphmatrix gm)
{
  int i;
  for(i=0;i<gm.vertexnum;i++)
  {
    gm.istrav[i]=0;
  }
  for(i=0;i<gm.vertexnum;i++)
  {
    if(gm.istrav[i]==0)
    {
      deeptraone9gm,i);
    }
  }
  System.out.printf("\n");
}
